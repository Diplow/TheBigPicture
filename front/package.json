    "eslint-plugin-redux-saga": "0.9.0",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "0.7.0",
    "file-loader": "1.1.11",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "imports-loader": "0.8.0",
    "jest-cli": "23.6.0",
    "lint-staged": "7.3.0",
    "node-plop": "0.16.0",
    "node-sass": "^4.7.2",
    "null-loader": "0.1.1",
    "plop": "2.1.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "react-test-renderer": "^16.5.2",
    "rimraf": "2.6.2",
    "sass-loader": "^7.0.1",
